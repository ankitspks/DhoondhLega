from .models import House
from django import forms

class HouseForm(forms.ModelForm):
	class Meta:
		model = Post
		fields = [
			"customer",
			"address",
			"description",
			"contact",
			"house_type",
			"price",
			"photos"
		]

class MoviesForm(forms.ModelForm):
	class Meta:
		model = Movies
		fields = [
			"name",
			"gener",
			"",
		]

